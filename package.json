{
  "name": "react-webpack-start",
  "version": "0.1.0",
  "description": "Webpack React Starter with Babel, ESLint, Mocha, Chai, Sinon, Enzyme",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run build && node dist/server.js",
    "start:dev": "npm run clean && node_modules/.bin/webpack-dev-server --inline --no-info",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:client": "node_modules/.bin/webpack --env.production --progress",
    "build:server": "node_modules/.bin/babel src/server.js --out-file dist/server.js",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "flow": "flow"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samouss/react-webpack-starter.git"
  },
  "author": [
    "Samuel Vaillant <samuel.vllnt@gmail.com>"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-module-resolver": "2.7.1",
    "babel-plugin-react-css-modules": "2.7.1",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "babel-preset-es2017": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.24.1",
    "css-loader": "0.28.4",
    "enzyme": "2.8.2",
    "enzyme-to-json": "1.5.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-loader": "1.7.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jest": "20.0.3",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "faker": "4.1.0",
    "flow-bin": "0.47.0",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "20.0.4",
    "lodash": "4.17.4",
    "postcss-loader": "2.0.5",
    "react-test-renderer": "15.5.4",
    "style-loader": "0.18.1",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "express": "4.15.3",
    "moment": "2.18.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.5",
    "react-router-dom": "4.1.1",
    "redux": "3.6.0",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "uuid": "3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "jest": {
    "moduleNameMapper": {
      "\\.css$": "identity-obj-proxy"
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
